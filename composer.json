{
    "name": "oyashiro846/phollection",
    "description": "PHP の標準配列だけで Scala のようなコレクション操作を扱えるようにする軽量ユーティリティです",
    "type": "library",
    "license": "MIT",
    "require": {
      "php": ">=8.4"
    },
    "autoload": {
        "psr-4": {
            "Oyashiro846\\Phollection\\": "src/"
        },
        "files": [
            "src/filter.php",
            "src/any.php",
            "src/tail.php",
            "src/init.php",
            "src/collect.php",
            "src/slice.php",
            "src/unique.php",
            "src/reduce.php",
            "src/map.php",
            "src/head_option.php",
            "src/last_option.php",
            "src/intersect.php",
            "src/map_keys.php",
            "src/merge.php"
        ]
    },
    "autoload-dev": {
      "psr-4": {
        "Oyashiro846\\Phollection\\": "tests/"
      }
    },
    "authors": [
        {
            "name": "Yashiro Hyrie",
            "email": "243130362+oyashiro846@users.noreply.github.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.4",
        "friendsofphp/php-cs-fixer": "^3.89"
    }
}
